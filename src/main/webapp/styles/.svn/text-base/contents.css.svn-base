/* Notify ----------------------------------------------*/
.notify{position:fixed;z-index:10000;right:30px;;min-width:300px;margin-top:150px;}
.notify ul li{padding:8px;margin-bottom:8px;box-shadow: rgba(0, 0, 0, 0.0980392) 0px 2px 4px;overflow:hidden;animation-duration:1s;}
.notify ul li .close a{display:block;top:10px;right:10px;width:20px;height:20px;font-size:0;background:url('../images/icon_popup.png') no-repeat -5px -35px;}
.notify ul li span, .notify ul li i, .notify ul li strong, .notify ul li em{margin:4px;}
.notify ul li em:before, .notify ul li em:after{content:" l ";color:rgba(0,0,0,0.2);margin:0 5px;}
.notify ul li strong{}
.notify ul li em{margin:4px;}
.notify ul li span{vertical-align:middle;}
/* Notify status */
.notify ul li i{color:#fff;border-radius:25px;padding:0 4px;margin:0 5px 0 0 ;font-size:0;vertical-align:middle;}
.notify ul li i:before{height:15px;font-size:11px;padding:8px 4px;margin:-5px 5px 0 -5px;vertical-align:middle;}
.notify ul li.cr{border:1px solid #e25353;background:#ff8181;color:#fff;}
.notify ul li.cr i:before{content:"Critical";display:inline-block;background:#dd1717;color:#fff;border-radius:25px;}
.notify ul li.ma{border:1px solid #e28253;background:#ffab81;}
.notify ul li.ma i:before{content:"Major";display:inline-block;background:#f2690e;color:#fff;border-radius:25px;}
.notify ul li.mi{border:1px solid #e7b009;background:#ffeaa8;}
.notify ul li.mi i:before{content:"Minor";display:inline-block;background:#e7b009;color:#fff;border-radius:25px;}
.notify ul li.wa{border:1px solid #3b96d6;background:#86bff0;}
.notify ul li.wa i:before{content:"Warning";display:inline-block;background:#4896f0;color:#fff;border-radius:25px;}
.notify ul li.cl{border:1px solid #7cdd77;background:#bcf5bc;}
.notify ul li.cl i:before{content:"Cleared";display:inline-block;background:#78b84d;color:#fff;border-radius:25px;}
.notify ul li.nor{border:1px solid #9288f1;background:#b8b0ff;}
.notify ul li.nor i:before{content:"Normal";display:inline-block;background:#9486df;color:#fff;border-radius:25px;}
.notify ul li.ind{border:1px solid #aaaaaa;background:#d8d8d8;}
.notify ul li.ind i:before{content:"Indeterminate";display:inline-block;background:#8c8f99;color:#fff;border-radius:25px;}

/* notify animated bounceInRight */
@-webkit-keyframes bounceInRight {
  0%, 60%, 75%, 90%, 100% {
    -webkit-transition-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
            transition-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
  }
  0% {
    opacity: 0;
    -webkit-transform: translate3d(3000px, 0, 0);
            transform: translate3d(3000px, 0, 0);
  }
  60% {
    opacity: 1;
    -webkit-transform: translate3d(-25px, 0, 0);
            transform: translate3d(-25px, 0, 0);
  }
  75% {
    -webkit-transform: translate3d(10px, 0, 0);
            transform: translate3d(10px, 0, 0);
  }
  90% {
    -webkit-transform: translate3d(-5px, 0, 0);
            transform: translate3d(-5px, 0, 0);
  }
  100% {
    -webkit-transform: none;
            transform: none;
  }}
@keyframes bounceInRight {
  0%, 60%, 75%, 90%, 100% {
    -webkit-transition-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
            transition-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
  }
  0% {
    opacity: 0;
    -webkit-transform: translate3d(3000px, 0, 0);
            transform: translate3d(3000px, 0, 0);
  }
  60% {
    opacity: 1;
    -webkit-transform: translate3d(-25px, 0, 0);
            transform: translate3d(-25px, 0, 0);
  }
  75% {
    -webkit-transform: translate3d(10px, 0, 0);
            transform: translate3d(10px, 0, 0);
  }
  90% {
    -webkit-transform: translate3d(-5px, 0, 0);
            transform: translate3d(-5px, 0, 0);
  }
  100% {
    -webkit-transform: none;
            transform: none;
  }}
.bounceInRight {
  -webkit-animation-name: bounceInRight;
          animation-name: bounceInRight;}


/* control --------------------------------------------------------------------------------------*/
.cont_ctrl{margin:0;margin-top:-10px;padding:0 15px;}
.control_wrap{width:100%;display:inline-block;margin-bottom:30px;}
.control_wrap .left, .control_wrap .right{width:48.4%;float:left;margin:0 0.8%;}
.control > div{background:#fff;border-radius:7px;padding:14px 25px 0;}
.ctrl_head{padding:0 0px 10px 0;}
.ctrl_head .switch{height:35px;margint-top:20px;}
.ctrl_head em{padding-left:100px;margin-top:25px;vertical-align:middle;color:#8f9299;}
.ctrl_head > toggle-1{margin-top:-20px;}
.ctrl_cont{border-top:1px solid #e7e7e7;margin:0 -25px;padding:0 25px 15px;display:none;}
.ctrl_cont p{height:65px;text-align:center;padding:25px 0;font-size:16px;font-weight:bold;}
.ctrl_check{padding:25px 25px 20px 50px;background:#f3f3f3;border:1px solid #d5d9d8;border-radius:5px;}
.ctrl_check [type="checkbox"]:checked + label, .ctrl_check [type="checkbox"]:not(:checked) + label{margin-bottom:5px !important;}
.ctr_list .sml{height:250px;overflow:auto;}
.ctr_list table td{word-break:break-all;text-overflow:initial !important;white-space:initial !important;overflow:visible !important;}
.toggle-round + label{width:0;height:0;}
.ctrl_check [type="checkbox"] + label{margin-right:20px !important;}
.cont_ctrl .tbl_list th:first-child, .cont_ctrl .tbl_list td:first-child{padding-left:25px;}

@media screen and (max-width: 1600px) {
    .control_wrap{margin-right:0;padding-right:0;}
	.control_wrap .left, .control_wrap .right{width:100%;}
}

/* Tree - menu authority & menu manager -----------------*/
article.box_2cols{}
article.box_2cols header{height:46px;line-height:46px;padding-left:25px;font-size:14px;color:#8b939e;width:initial;}

@media screen and (max-width:1450px){
article.box_2cols .boxOne header{font-size:0px;}
article.box_2cols .boxOne:nth-child(1) header{font-size:14px;}
}

.boxOne{float:left;width:50%;height:650px;background:#fff;border-radius:7px;}
.boxOne:first-child{border-right:solid 2px #e5e5e5;}
.boxLeft{float:left;width:34%;height:650px;background:#fff;border-radius:7px;border-right:solid 2px #e5e5e5;overflow:hidden;}
.boxRight{float:left;width:66%;height:650px;background:#fff;border-radius:7px;margin-bottom:85px;}
.boxMid{position:absolute;display:none;}
.boxMid .arrow{display:block;margin-left:-18px;margin-top:300px;width:34px;height:38px;border-radius:7px;border:solid 1px #e5e5e5;background:#f2f2f2 url('../images/menu_authority.png') -206px -158px no-repeat;}

.boxRight .detail{border-top:solid 1px #e5e5e5;border-radius:0;height:604px;background:none;}
.boxRight .detail tr:last-child td{border-bottom:0;}
.boxRight .detail .switch{margin-right:8px;}
.boxRight .detail .desc{display:inline-block;width:200%;height:265px !important;overflow-y:auto;}
.boxRight .btn_area{float:right;margin-top:22px;}

article .treeCon{float:right;display:inline-block;margin-right:8px;}
article .treeCon li{float:left;}
article .treeCon a{display:inline-block;width:36px;height:40px;margin:0;text-align:center;font-size:0;background:url('../images/menu_authority.png') -7px -2px no-repeat;}
article .treeCon a:hover {color:#000;}
article .treeCon .full {background-position: -57px -2px;}
article .treeCon .close {background-position: -107px -2px;}
article .treeCon .group {background-position: -157px -2px;}
article .treeCon .menu {background-position: -207px -2px;}
article .treeCon .del {background-position: -7px -2px;}
article .treeCon .full:hover,
article .treeCon .close:hover,
article .treeCon .group:hover,
article .treeCon .menu:hover,
article .treeCon .del:hover{background-position-y:-52px;}
article .treeCon .full:active,
article .treeCon .close:active,
article .treeCon .group:active,
article .treeCon .menu:active,
article .treeCon .del:active{background-position-y:-102px;}
article .treeCon .all,
article .treeCon .read {background:none;font-size:14px;font-weight:600;line-height:1.25em;color:#989da6;padding-top:13px;}
article .treeCon .all:before {content:"A";display:inline-block;width:19px;height:20px;border-radius:3px;border:solid 1px #989da6;}
article .treeCon .read:before {content:"R";display:inline-block;width:19px;height:20px;border-radius:3px;border:solid 1px #989da6;}
article .treeCon .all:hover:before,
article .treeCon .read:hover:before{color:#039adf;border:solid 1px #039adf;}
article .treeCon .all:active:before,
article .treeCon .read:active:before{color:#2a3139;border:solid 1px #2a3139;}

/*tree*/
