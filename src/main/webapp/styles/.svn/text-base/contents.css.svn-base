#snb .left-menu.f-menu{padding-top:50px !important; margin-top:0px; transition:transform 0.5s;}

/* cont_body */
.cont_body{padding:35px 35px;margin-bottom:-20px;word-break:break-all;overflow:hidden;}
.cont_body header {display:inline-block;width:100%;padding-bottom:8px;}
.cont_body header h3{display:inline-block;padding-right:18px;}
.cont_body header:first-child h3{margin-top:0;}
.cont_body header p{display:inline-block;color:#8b939e;font-size:12px;margin-left:2px;margin-bottom:7px;}
.cont_body header .ms-parent.type3{margin-top:-6px;margin-right:15px;} /*h3 옆에 select box가 있는 경우*/
.cont_body header .btn_area{float:right;margin:31px 10px 0 0;}
.cont_data{padding:0px;height:100%;}

.cont_top_right{position:absolute;right:0;margin-top:-40px;margin-right:35px;text-align:right;}
.cont_top_right .select_single,
.cont_top_right .select_multi.type3,
.cont_top_right .select_group.type3{display:inline-block;margin-left:25px;}
.cont_top_right .select_single label{font-weight:600;}
.cont_top_right .download{display:inline-block;margin:0 0 0 15px;}

.cont_body header:first-child .cont_top_right{top:0;}
.cont_body header .cont_top_right{float:right;position:relative;margin:0;top:34px;}
.cont_body header .cont_top_right .select_single:first-child{margin-right:15px;} 
.cont_body header .cont_top_right .ms-parent.type3{margin-top:0;margin-right:0;}

.util_btn{position:relative;right:30px;top:-50px;height:0;float:right !important;border:none !important;background:none !important;}
.util_btn button{width:30px !important;padding:0 !important;min-width:25px !important;}
.util_btn .dropbtn{display:inline-block;text-align:center;height:30px;font-size:0;background:url('../images/icon_btn.png') -597px 1px no-repeat;}
.util_btn .dropdown:hover .dropbtn{background:url('../images/icon_btn.png') -597px -49px no-repeat;}
.util_btn .dropdown{display:inline-block;}
.util_btn .dropdown_content{display:none;width:70px;position:absolute;z-index:100;margin-left:-17px;background-color:#f9f9f9;box-shadow:2px 4px 9px -1px rgba(0,0,0,0.2);border:1px solid #aaaeb3;border-radius:5px;overflow:hidden;}
.util_btn .dropdown_content a{color:black;padding:12px 16px;text-decoration:none;display:block;text-align:center;}
.util_btn .dropdown_content a:hover{background-color:#f1f1f1}
.util_btn .dropdown:hover .dropdown_content{display:block;}
.util_btn .dropdown_content li{border-top:1px solid #e0e3e6;}
.util_btn .dropdown_content li:first-child{border-top:none;}

/* search area */
.search_area{width:100%;background:url('../images/bg_dotted.png') repeat ;min-height:85px;padding:0 15px;display:inline-block;box-shadow:0px 1px 5px 2px rgba(0,0,0,0.09);margin-bottom:-4px;}
.search_area > div.change{width:86.9%;}

.search_area .ms-parent.single button.ms-choice {box-sizing:initial !important;width:110% !important;padding-left:1.5em;display:inline-block !important;}
.search_area .ms-parent.single button.ms-choice span{box-sizing:initial !important;width:110%;}
.search_area .ms-parent button:hover{background:red;}
.search_area .ms-parent.single{box-sizing:initial !important;padding-right:2em;margin-right:25px !important;}

@media screen and (max-width: 1900px)and (min-width: 1750px) {
.search_area > div{width:1700px;}
}

.search_area > div > div{float:left;height:85px;padding:20px;background:url('../images/bg_dotted_y.png') no-repeat right;}
.search_area > div > div > label{display:block;font-size:12px;color:#889198;font-weight:bold;margin-bottom:-22px;}
.search_area > div > div > label:after{content:':';padding-left:10px;}
.search_area .ms-parent{margin-top:-6px!important;}
.search_area .group .ms-drop, .search_area .group_single .ms-drop{width:602px;}
.search_area .ms-parent.group, .search_area .ms-parent.group_single{width:240px !important;}
.ms-drop [type="checkbox"]:not(:checked), .ms-drop [type="checkbox"]:checked{position:relative;width:15px;height:15px;}
.group_single .ms-drop label{padding-left:10px;cursor:pointer}
.group_single li.group:hover{background:none !important;}
.group_single .ms-drop label.optgroup{cursor:default !important;}

.search_area button.ms-choice{background:url('../images/icon_select.png') right 18px no-repeat !important;}
.search_area button:focus.ms-choice{background-position:right -60px !important;}
.select_group{width:280px;}
.select_single{display:inline-block;}
.single label{margin-left:-30px;}
.select_keyword .ms-parent{width:120px !important;margin-top:-5px;}
.select_keyword input{background:none !important;border:none !important;width:150px;height:40px;padding-top:30px;margin-left:7px;color:#2a3139;font-size:18px;vertical-align:middle;}

/* search button */
button.srch{float:left;background:url('../images/btn_search.png') no-repeat;width:46px;height:46px;min-width:46px;margin-top:25px;margin-left:15px;padding:0;font-size:0;}
button.srch:hover{background:url('../images/btn_search.png') 0 -50px no-repeat;}
button.srch:active{background:url('../images/btn_search.png') 0 -100px no-repeat;}

.filter{height:54px;}
.filter.type2{border-bottom:dotted 1px #c2c5ca;margin-bottom:-15px;}
.filter .select_single{margin:0 13px;}
.filter div:first-child.select_single{margin:0 13px 0 2px;}
.filter .select_single > label{font-weight:600;}
.filter button.srch{float:none;width:30px;height:30px;min-width:30px;margin:0;padding:0;background:url('../images/icon_search.png') -60px -2px no-repeat;}
.filter button.srch:hover{background-position-y:-37px;}
.filter button.srch:active{background-position-y:-72px;}

/* table -------------------------------------------------------*/
.detail,.list{border-radius:7px;background:#fff;}
table{width:100%;table-layout:fixed;}
table th, table td{padding:0 20px 0 20px;border-bottom:1px solid #e7e7e7;text-align:left;}
table th{height:50px;color:#8b939e;border-left:1px solid #e7e7e7;}
table th:first-child {border-left:0;}
table td{height:55px;font-size:16px;font-weight:600;}
table td.no{padding:0 25px !important;}
table .no{width:125px;text-align:center;}
table .scl{width:10px;border-left:0 !important;}/*scroll cell width*/
table .t_c{text-align:center;}
table .t_r{text-align:right;}
table .chk{width:70px;}
table .chk label{font-size:0 !important;}
table .chk [type="checkbox"]:not(:checked) + label:before,
table .chk [type="checkbox"]:checked + label:before,
table .chk [type="checkbox"]:not(:checked) + label:after,
table .chk [type="checkbox"]:checked + label:after{top:-12px;}
table .date{width:188px;}
table .scr{width:10px;} 
table .ico{width:85px;}
table td.ico > button{display:block;min-width:28px;height:28px;padding:0;font-size:0;}
table button.more{background:url('../images/icon_monitoring.png') -407px -1px no-repeat;}
table button.more:hover{background-position:-407px -41px;}
table button.more:active{background-position:-407px -81px;}
table .per{display:inline-block;width:42px;}
table td.ok{font-size:0 !important;text-align:center;}
table td.ok:before{content:"";display:inline-block;min-width:15px;height:15px;border-radius:50px;border:solid 3px #fff;margin-right:7px;}

.sort{cursor:pointer;}
.sort:after{content:"";display:inline-block;float:right;position:relative;width:15px;height:17px;background:url('../images/icon_btn.png') -454px -3px no-repeat;vertical-align:middle;margin-left:5px;}
.sort.up:after{background-position:-504px -3px;}
.sort.down:after{background-position:-554px -3px;}
.sort:hover:after{background-position:-454px -53px;}
.sort.up:hover:after{background-position:-504px -53px;}
.sort.down:hover:after{background-position:-554px -53px;}
table th.sort.up, table th.sort.down{color:#222;font-weight:600;background:#f3f3f3;}
table th.sort:hover,table th.sort.up:hover, table th.sort.down:hover{background:#ebeced;}


.cont_left{float:left;width:360px;margin-bottom:30px;}
.cont_right{float:left;padding-left:30px;width:calc(100% - 360px);height:481px;border-left:1px solid #d6d6d7;} 

.cont_left_half{float:left;width:50%;}
.cont_right_half{float:left;width:50%;padding-left:30px;}

@media screen and (max-width:1450px){
.cont_right_half header p{display:none;}
}

.cont_left_half .detail{height:650px;}
.cont_left_half .detail table tr:last-child td{border-bottom:0;}

.cont_right>header{float:left;width:100%;}
.cont_right>header h3 em{color:#00b37d}
.cont_right>table{width:100%;table-layout:fixed;white-space:nowrap;}
.cont_right .scroll{height:400px;} 
.cont_right .cont_top_right{margin-top:-5px;}

.cont_left .detail{height:448px;margin-right:30px;}
.cont_left .detail.type2{height:680px;}
.cont_left .detail .catagory ul li{width:140px;float:left;margin-bottom:8px}
.cont_left .detail .catagory label{width:140px;}
.cont_left .detail table td{padding:8px 25px 10px 25px;}
.cont_left .detail table td .ms-parent{margin-bottom:6px;}
.cont_left .detail input[type="radio"]:not(:checked) + label,.detail.type02 input[type="radio"]:checked + label{margin-right:14px;}
.cont_left .detail table tr:last-child td{border-bottom:0;}
.cont_left .detail .btn_area{text-align:center;margin-top:20px}

.command{height:342px;width:100%;background:#0a1e40;border-radius:7px;margin-bottom:30px;overflow:hidden;}
.command header{width:100%;height:42px;background:#122d5a;}
.command header h3{color:#00c588;font-size:14px;font-weight:normal;padding:13px 25px;}
.command header .btn_area{float:right;position:relative;width:133px;text-align:right;padding-right:17px;margin:0;top:-5px;}
.command .statusview{width:100%;color:#fff;padding:15px 25px;height:calc(100% - 42px);overflow:auto;line-height:1.4em;}
.command.type2{height:680px;float:left;width:calc(100% - 360px);}
.command.type2 .left{height:638px;width:310px;display:inline-block;border-right:1px solid #020711;}
.command.type2 .right{height:638px;width:calc(100% - 310px);float:right;}

/* detail table */
.detail table{table-layout:auto;}
.detail table th, table td{padding:15px 25px;}
.detail table th{height:34px;border-bottom:0;padding-bottom:0px;border-right:1px solid #e5e5e5;color:#8b939e;}
.detail table td{height:51px;border-right:1px solid #e5e5e5;padding:8px 25px 20px 25px;}
.detail table th:last-child{border-right:0;}
.detail table td:last-child{border-right:0;}
.detail table th span{font-size:12px;}
.detail table button.btn_stroke{padding:0 5px;margin-left:5px;ㅡㅑ}
table td .ms-parent{width:100% !important;}
table td .ms-parent button{margin-top:0;width:100% !important;height:38px;padding:5px 15px 5px 15px;border-radius:100px;border:solid 2px #d5dae0;margin-left:0;
background-color:#d5dae0 !important;
background-image:url(../images/select_arrow.png) !important;
background-position:right !important;
background-repeat:no-repeat !important;}
/*td select*/
table td .ms-choice > span{margin-top:-1px;margin-left:9px;font-size:16px;font-weight:600;color:#222;}
th.imp:after {content:"*";padding-left:2px;color:#019ae0;font-family:Verdana;}
/* register table */
.register td{height:65px !important;}

.detail table td .switch{margin-top:-30px;}
.detail table tr td.slidearea{padding:0 8%;}
.detail table.rangeslide th, table.rangeslide td{padding:0;}
.detail table.rangeslide td{height:8px;}

.list{clear:both}
.list .tbl_update th{border-left:1px solid #e7e7e7;}
.list .tbl_update td{padding:10px 15px;}
.list .tbl_update td:first-child{padding:10px 25px;}
.list .tbl_update th:first-child,.list .tbl_update th:last-child{border-left:0;}
.list .tbl_update tbody tr:last-child td{padding:10px 15px 15px 15px;}
.list .tbl_update tbody tr:last-child td:first-child{padding:10px 25px;}
.list .btn_area{float:right;margin-top:-35px;}

/* list table */
.data{overflow-x:auto;border-bottom:1px solid #d7d8d9;}
.data .tbl_list th{height:45px;padding:0 20px 0 20px;background:#dfe1e2;color:#6c7273;border-top:1px solid #93989d;border-left:1px solid rgba(147,152,157,.15);border-bottom:1px solid #c9ccce;}
.data .tbl_list th:first-child{border-left:0;padding-left:30px;}
.data .tbl_list th:last-child{padding:0;}
.data .tbl_list td{height:50px;padding:0 20px 0 20px;background:#f3f3f3;border-bottom:1px solid #d7d8d9;font-size:14px;text-overflow:ellipsis;white-space:nowrap;overflow:hidden;}
.data .tbl_list td:first-child{padding-left:30px;}
.data .tbl_list tr:last-child td{border-bottom:none;}
.data .tbl_list tr:hover td{background:#fff;}
.data .count{text-align:right;padding-right:35px !important;}
.data .scroll{min-height:300px;}

/* list table - type02 (box design) */
.data.type02{overflow-x:auto;border-radius:5px;border:1px solid #babdbf;margin-bottom:-15px;}
.data.type02 .tbl_list th{border-top:0;text-overflow:ellipsis;white-space:nowrap;overflow:hidden;}
.data.type02 .tbl_list th:first-child,
.data.type02 .tbl_list td:first-child{padding-left:20px;}
