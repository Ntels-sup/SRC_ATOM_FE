button.btn_type2.major:hover{background:#e5f7ff;}
button.btn_type2:active{background:#373f48;color:#fff;border:1px solid #373f48;}

button.btn_icon{width:32px;height:32px;min-width:0;padding:0;margin:12px 5px;font-size:0;border-radius:3px;background:url('../images/icon_btn.png') no-repeat;}
