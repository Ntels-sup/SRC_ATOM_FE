@charset "utf-8";

/* Layout -------------------------------------------------------*/
.wrap{width:100%;height:100%;background:#e5e5e5;}
.header{background:#373f48;min-width:1260px;height:120px;}
.content{width:100%;left:0;top:0;min-width:1260px;padding-left:0px;background:#e5e5e5;transition-duration:0;}
.content.manager{height:calc(100% - 120px);}
.content.change{padding-left:235px;transition-duration:0.2s;}

/* header */
/* alarm */
/* gnb  -------------------------------------------------------*/
.nav-container{background:#374049;height:75px;padding-top:15px;transition-duration:0.4s;}
.f-nav{position:fixed;height:50px;padding-top:5px;left:0;top:0;width:100%;min-width:1260px;z-index:100;}
.nav{height:50px;float:left;}

.gnb_logo{width:200px;position:absolute;left:0px;display:none;}
.gnb_logo h1{position:absolute;top:0px;left:70px;width:151px;height:74px;background:url('../images/logo.png') no-repeat;}
.gnb_logo h1 .logo{font-size:0px;}
.gnb_logo h1.change{background:url('../images/logo_sliding.png') no-repeat;}
.gnb_logo h1.action{transition-duration:0.5s;}
.gnb_logo .f-h1{margin-top:-10px; !important;transition-duration:0.5s;}
.gnb_logo .trigger-menu{position:absolute;left:10px;top:20px;}

.nav-container ul{list-style-type:none;margin:-7px;padding:0;margin-left:200px;transition-duration:0.4s;}
.nav-container > ul.change{margin-left:255px;transition-duration:0.2s;}
.nav-container ul ul{margin-left:inherit;}
.nav-container li{float:left;padding:0 6px;}
.nav-container li a, .nav-container .dropbtn{display:inline-block;color:#9aa4b0;text-align:center;padding:16px 16px;text-decoration:none;font-weight:600;text-transform:uppercase;}
.nav-container li a:hover, .nav-container .dropdown:hover .dropbtn{background:none;color:#fff;}
.nav-container li a:focus{background:none;}
.nav-container li.dropdown{display:inline-block;visibility:;}

.nav-container .dropdown_content, 
.nav-container .dropdown_content_3dpt{display:none;position:absolute;z-index:100;background-color:#374049;min-width:185px;box-shadow:0px 1px 9px -1px rgba(0,0,0,0.56);border-radius:5px;padding:7px 0 10px 0;}
.nav-container .dropdown_content{margin-top:-7px;}
.nav-container .dropdown_content_3dpt{margin-left:95%;margin-top:-41px;}
.nav-container .dropdown_content a{color:#dadfe5 !important;padding:8px 20px;text-decoration:none;display:block;text-align:left;font-weight:normal;text-transform:capitalize;}
.nav-container .dropdown_content a:hover,
.nav-container .dropdown_content .dropdown_sub a:hover{background:#2a3139;color:#00b37c !important;}
.nav-container .dropdown:hover .dropdown_content, 
.nav-container .dropdown_sub:hover .dropdown_content_3dpt{display:inline-block;}
.nav-container .dropdown_sub a.dropbtn:after{content:"";display:inline-block;width:7px;height:14px;background:url(../images/menu_arrow.png) no-repeat;margin-top:3px;float:right;}
.nav-container .dropdown_sub a.dropbtn:hover:after{background-position:-7px 0;}

.nav-container .dropdown_content li, .nav-container .dropdown-content_3dpt li{width:100%;padding:0;}
.nav-container .dropdown_content li a:before{display:none !important;}

.nav-container .nav li a:before{content:"";display:inline-block;width:40px;height:28px;background:url(../images/icon_top_menu.png) 0 -6px no-repeat;margin-right:7px;vertical-align:middle;}
.nav-container .nav li:nth-child(2) a:before{background-position-x:-40px;}
.nav-container .nav li:nth-child(3) a:before{background-position-x:-80px;}
.nav-container .nav li:nth-child(4) a:before{background-position-x:-120px;}
.nav-container .nav li:nth-child(5) a:before{background-position-x:-160px;}
.nav-container .nav li:nth-child(6) a:before{background-position-x:-200px;}
.nav-container .nav li:nth-child(7) a:before{background-position-x:-240px;}
.nav-container .nav li a:hover:before,
.nav-container .nav .dropdown:hover a:before{background-position-y:-46px;}

@media screen and (max-width: 1700px){
.nav-container .nav li a:before{width:0;margin-right:0;background:none;}
}

@media screen and (max-width: 1360px){
.nav-container li{float:left;padding:0 0 0 6px;}
.nav-container li a{font-size:0;}
.nav-container .nav li a:before{width:40px;margin-right:-6px;background:url(../images/icon_top_menu.png) 0 -6px no-repeat;}
.nav-container .dropdown_content a,
.nav-container .dropdown_content_3dpt a{font-size:14px;}
}
@media screen and (max-width: 1100px){
.nav_info{display:none;}	
}

/* gnb information */
/*.nav_info{padding-right:5px;margin-top:8px;background:#373f48;float:right;}*/
.nav_info{position:absolute;right:5px;margin-top:8px;background:#373f48;}
.nav_info p{width:55px;float:left;}
.nav_info p span{display:inline-block;width:25px;height:25px;font-size:0;background:url(../images/icon_top.png) 0 -25px no-repeat;vertical-align:middle;}

.nav_info p.id{min-width:130px;margin-top:-15px;text-align:right;float:right;}
.nav_info p.id strong{display:inline-block;color:#9fa2a6;font-size:13px;font-weight:normal;vertical-align:middle;padding-top:6px;}
.nav_info p.alarm span{background-position:-25px 0;}
.nav_info p.help a span{background-position:-50px 0;}
.nav_info p.menu a span{background-position:-75px 0;}
.nav_info p.id a:hover span{background-position:0 -25px;}
.nav_info p.help a:hover span{background-position:-50px -25px;}
.nav_info p.menu a:hover span{background-position:-75px -25px;}

.nav_info p.alarm em{display:inline-block;border-radius:100px;background:#ffff34;color:#373f48;font-size:11px;letter-spacing:-1px;padding:1px 4px 0 2px;min-width:16px;height:16px;font-weight:bold;margin-top:-7px;margin-left:-10px;vertical-align:text-top;}

.nav_info .dropdown{display:inline-block;float:left;width:150px;height:35px;cursor:default;margin-right:40px;}
.nav_info .dropbtn{display:inline-block;text-align:center;width:25px;height:25px;font-size:0;}
.nav_info .dropdown_content{display:none;position:absolute;margin-left:20px;margin-top:35px;padding:0;z-index:100;background-color:#f9f9f9;min-width:165px;box-shadow:0 2px 5px rgba(0, 0, 0, .15);border:1px solid #aaaeb3;}
.nav_info .dropdown_content a{color:black;padding:12px 16px;text-decoration:none;display:block;text-align:left;}
.nav_info .dropdown_content a:hover{background-color:#f1f1f1}
.nav_info .dropdown:hover .dropdown_content{display:block;}

.nav_info .dropdown li:first-child{color:#00c9ee;font-weight:bold;padding:14px 20px;border-top:none;}
.nav_info .dropdown li:first-child span{color:#909499;font-size:10px;}
.nav_info .dropdown li{border-top:1px solid #e0e3e6;}
.nav_info .dropdown li a{padding:10px 20px;}
.nav_info .dropdown li a:before{content:"";display:inline-block !important;width:25px;height:25px;background:url('../images/icon_top.png') no-repeat !important;vertical-align:middle;margin-right:7px;}
.nav_info .dropdown li:nth-child(2) a:before{background-position:0 -50px !important;}
.nav_info .dropdown li:nth-child(3) a:before{background-position:-25px -50px !important;}
.nav_info .dropdown li:nth-child(4) a:before{background-position:-50px -50px !important;}

/* title area */
.page_title{background:#2a3139;display:inline-block;width:100%;height:45px;padding:12px 30px 12px 37px;}
.page_title h2{font-size:16px;min-width:25%;float:left;color:#00c188;padding-left:0;transition-duration:0;}
.page_title h2.change{padding-left:235px;transition-duration:0.2s;}
.page_title ul{float:right;display:inline-block;}
.page_title ul p{float:left;color:#858b90;margin-right:7px;}
.page_title ul li{float:left;padding:5px 10px 7px 10px;margin:-5px 4px;background:#3f4752;border-radius:25px;}
.page_title ul li a{color:#bdc4ce;}
.page_title ul li:hover, .page_title ul li:focus{background:#00c188;color:#fff;}
.page_title ul li:active{background:#01ae7b;}
.page_title ul li:hover a{color:#fff;}
.page_title a.x:after{content:'';width:11px;height:11px;display:inline-block;margin-left:10px !important;margin-top:-2px;background:url('../images/icon_close.png') no-repeat;vertical-align:middle;}
.page_title a:hover.x:after{background-position:0 -11px;}
.page_title .x span{display:none;}

/* snb */
#snb{position:fixed;left:-235px;height:100%;background:#fff;z-index:9999;}
#snb.sliding{}
#snb.change{margin-top:0px}
#snb h1{position:absolute;left:305px;width:151px;height:74px;background:url('../images/logo.png') no-repeat;z-index:9999;}
#snb h1 .logo{font-size:0px;}
#snb h1.change{background:url('../images/logo_sliding.png') no-repeat;}
#snb h1.action{transition-duration:0.5s;}
#snb .f-h1{margin-top:-10px; !important;transition-duration:0.5s;}

#snb .trigger-menu{position:absolute;left:243px;margin-top:20px;transition-duration:0.5s;z-index:9999;}
