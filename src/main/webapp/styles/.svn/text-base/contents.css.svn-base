/* error & chrome download*/
.error, .chrome{width:100%;height:100%;min-width:820px;background:#fff;text-align:center;}
.error header, .chrome header{display:inline-block;width:100%;margin-bottom:90px;border-bottom:solid 1px #e7e7e7;}
.error header h1, .chrome header h1{float:left;width:200px;height:40px;font-size:0;background:url('../images/logo_img2.png') no-repeat;}
.error header span, .chrome header span{float:right;font-size:12px;color:#2a3139;padding:9px 5px;}
.error button, .chrome button{height:60px;font-size:22px;font-weight:normal;color:#fff;padding:0 35px;background:#039adf;border-radius:5px;}
.error button:hover, .chrome button:hover{background-color:#0080bf;}
